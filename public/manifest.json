  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+soulo",
      "url": "/app?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/app/journal",
      "accept": {
        "audio/*": [".mp3", ".wav", ".m4a", ".webm"]
      }
    }
  ],
  "share_target": {
    "action": "/app/journal",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "audio",
          "accept": ["audio/*"]
        }
      ]
    }
  }
